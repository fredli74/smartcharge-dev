    "apollo-link-ws": "^1.0.19",
    "apollo-server-express": "^2.9.13",
    "commander": "^2.20.3",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.4.8",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "moment": "^2.24.0",
    "pg-promise": "^9.3.6",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.6.2",
    "type-graphql": "^0.17.5",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-ctk-date-time-picker": "^2.4.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.13",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
