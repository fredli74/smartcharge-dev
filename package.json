{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:server && npm run build:app",
    "lint": "vue-cli-service lint",
    "_babel": "npx babel --extensions \".ts\" --copy-files --verbose --ignore **/node_modules --config-file=./babel.config.js",
    "_babel:providers": "npm run _babel -- providers --out-dir dist/providers",
    "_babel:shared": "npm run _babel -- shared --out-dir dist/shared",
    "_babel:server": "npm run _babel -- server --out-dir dist/server",
    "start:worker": "node dist/server/agency.js -d INTERNAL_SERVICE_TOKEN http://localhost:3000",
    "start:server": "node dist/server/server.js",
    "build:server": "npm run _babel:shared && npm run _babel:providers && npm run _babel:server",
    "build:app": "vue-cli-service build"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "apollo-boost": "^0.4.3",
    "apollo-link-context": "^1.0.18",
    "apollo-link-ws": "^1.0.18",
    "apollo-server-express": "^2.8.1",
    "commander": "^2.20.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.1.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.0",
    "pg-promise": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.6.2",
    "type-graphql": "^0.17.4",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.1.1",
    "vuetify": "^2.0.5",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@mdi/font": "^3.9.96",
    "@types/compression": "0.0.36",
    "@types/connect-history-api-fallback": "^1.3.2",
    "@types/dotenv": "^6.1.1",
    "@types/graphql-type-json": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-pwa": "^3.10.0",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "apexcharts": "^3.8.4",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "babel-eslint": "^10.0.1",
    "deepmerge": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "typescript": "^3.5.3",
    "vue-apexcharts": "^1.4.0",
    "vue-apollo": "^3.0.0-rc.2",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  }
}
